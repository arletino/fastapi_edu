curl -X 'POST' \
'http://127.0.0.1:8000/users/'\
-H 'accept: application/json'\
-H 'Content-Type: application/json'\
-d '{
    'name': 'Alex',
    'email':'alex@mail.ru'
}'

url -X 'POST' 'http://127.0.0.1:8080/users/' -H 'accept: application/json' -H 'Content-Type: application/json' -d '{"name": "Alex", "email":"alex@mail.ru"}